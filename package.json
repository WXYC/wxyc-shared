{
  "name": "@wxyc/shared",
  "version": "0.1.0",
  "description": "Shared DTOs, test utilities, and E2E tests for WXYC services",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./dtos": {
      "types": "./dist/dtos/index.d.ts",
      "import": "./dist/dtos/index.js",
      "default": "./dist/dtos/index.js"
    },
    "./test-utils": {
      "types": "./dist/test-utils/index.d.ts",
      "import": "./dist/test-utils/index.js",
      "default": "./dist/test-utils/index.js"
    },
    "./auth-client": {
      "types": "./dist/auth-client/index.d.ts",
      "import": "./dist/auth-client/index.js",
      "default": "./dist/auth-client/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "prepare": "npm run build",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "test:setup-script": "bats scripts/__tests__/setup-dev-environment.test.sh",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^22.0.0",
    "bats": "^1.11.0",
    "tsup": "^8.4.0",
    "typescript": "^5.6.2",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "keywords": [
    "wxyc",
    "dto",
    "testing"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WXYC/wxyc-shared.git"
  },
  "license": "MIT",
  "dependencies": {
    "better-auth": "^1.4.9",
    "date-fns": "^4.1.0"
  }
}
